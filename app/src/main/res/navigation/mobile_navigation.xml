<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.grandmarket.ui.home.HomeFragment"
        android:label="@string/title_home1"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.grandmarket.ui.dashboard.DashboardFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.grandmarket.ui.notifications.NotificationsFragment"
        android:label="@string/title_cart"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_blank"
        android:name="com.example.grandmarket.BlankFragment"

        android:label="@string/title_profile"
        tools:layout="@layout/fragment_blank" />
</navigation>